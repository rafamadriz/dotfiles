{
    "extractor": {
        "base-directory": "~/Pictures/random/gallery-dl",
        "archive": "~/Pictures/random/gallery-dl/db/archive.sqlite3"
    },
    "postprocessors": [
        {
            "name": "exec",
            "event": "after",
            "filter": "extension in ('jpg','jpeg','png')",
            "command": "sh -c 'avifenc --speed 7 \"$1\" \"${1%.*}.avif\" && rm \"$1\"' sh {}",
            "mtime": true
        }
    ]
}
